{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["es6", "esnext.asynciterable"],
    "moduleResolution": "node",
    "module": "commonjs",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist"
  }
}
